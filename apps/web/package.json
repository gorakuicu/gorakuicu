{
  "name": "@gorakuicu/web",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:remix": "cross-env NODE_ENV=production remix build",
    "build:worker": "esbuild ./app/entry.workbox.ts --outfile=./public/entry.workbox.js --minify --bundle --platform=node --format=esm --define:process.env.NODE_ENV='\"production\"'",
    "dev": "run-p dev:*",
    "dev:remix": "cross-env NODE_ENV=development remix dev",
    "dev:worker": "esbuild ./app/entry.workbox.ts --outfile=./public/entry.workbox.js --bundle --platform=node --format=esm --define:process.env.NODE_ENV='\"development\"' --watch",
    "start": "remix-serve build",
    "typecheck": "tsc",
    "test": "vitest --config ./vitest.config.ts --run",
    "coverage": "vitest run --coverage",
    "lint": "eslint -c ./.eslintrc.js --ignore-path \"../../.eslintignore\" .",
    "check": "npm run typecheck && npm run lint",
    "format": "prettier --config \"../../.prettierrc.json\" --ignore-path \"../../.prettierignore\" --write \"./**/*.{js,ts,tsx,json,css,md,yml,html,mdx}\"",
    "lint:fix": "npm run lint -- --fix",
    "clean:basics": "rimraf .cache storybook-static reports",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build"
  },
  "dependencies": {
    "@nextui-org/react": "2.1.10",
    "@remix-run/css-bundle": "1.19.3",
    "@remix-run/node": "1.19.3",
    "@remix-run/react": "1.19.3",
    "@remix-run/serve": "1.19.3",
    "clsx": "2.0.0",
    "framer-motion": "10.16.3",
    "isbot": "3.6.13",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "4.10.1",
    "remix-utils": "6.6.0",
    "npm-run-all": "4.1.5",
    "cross-env": "7.0.3",
    "dotenv": "16.3.1",
    "web-push": "3.6.5",
    "@remix-pwa/sw": "1.1.4",
    "@remix-pwa/push": "1.0.2",
    "workbox-background-sync": "7.0.0",
    "workbox-routing": "7.0.0",
    "workbox-strategies": "7.0.0"
  },
  "devDependencies": {
    "@gorakuicu/eslint-config": "workspace:*",
    "@gorakuicu/tsconfig": "workspace:*",
    "@ngneat/falso": "7.1.0",
    "@remix-run/dev": "1.19.3",
    "@remix-run/testing": "1.19.3",
    "@storybook/addon-a11y": "7.4.0",
    "@storybook/addon-actions": "7.4.0",
    "@storybook/addon-controls": "7.4.0",
    "@storybook/addon-essentials": "7.4.0",
    "@storybook/addon-interactions": "7.4.0",
    "@storybook/addon-links": "7.4.0",
    "@storybook/addons": "7.4.0",
    "@storybook/api": "7.4.0",
    "@storybook/blocks": "7.4.0",
    "@storybook/builder-vite": "7.4.0",
    "@storybook/components": "7.4.0",
    "@storybook/core-events": "7.4.0",
    "@storybook/manager-api": "7.4.0",
    "@storybook/react": "7.4.0",
    "@storybook/react-vite": "7.4.0",
    "@storybook/testing-library": "0.2.0",
    "@storybook/theming": "7.4.0",
    "@testing-library/react": "14.0.0",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "autoprefixer": "10.4.15",
    "eslint-plugin-perfectionist": "1.5.1",
    "eslint-plugin-storybook": "0.6.13",
    "eslint-plugin-tailwindcss": "3.13.0",
    "happy-dom": "10.11.2",
    "postcss": "8.4.29",
    "storybook": "7.4.0",
    "storybook-addon-themes": "6.1.0",
    "storybook-dark-mode": "3.0.1",
    "storybook-mobile": "1.0.1",
    "tailwindcss": "3.3.3",
    "vite": "4.4.9",
    "vite-tsconfig-paths": "4.2.0",
    "vitest": "0.34.3"
  }
}
