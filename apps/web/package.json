{
  "name": "@gorakuicu/web",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "remix build",
    "dev": "remix dev",
    "start": "remix-serve build",
    "typecheck": "tsc",
    "test": "vitest --config ./vitest.config.ts --run",
    "coverage": "vitest run --coverage",
    "ladle": "ladle serve --theme dark",
    "lint": "eslint -c ./.eslintrc.json --ignore-path \"../../.eslintignore\" .",
    "check": "npm run typecheck && npm run lint",
    "format": "prettier --config \"../../.prettierrc.json\" --ignore-path \"../../.prettierignore\" --write \"./**/*.{js,ts,tsx,json,css,md,yml,html,mdx}\"",
    "lint:fix": "npm run lint -- --fix"
  },
  "dependencies": {
    "@nextui-org/react": "2.1.7",
    "@remix-run/css-bundle": "1.19.3",
    "@remix-run/node": "1.19.3",
    "@remix-run/react": "1.19.3",
    "@remix-run/serve": "1.19.3",
    "framer-motion": "10.16.2",
    "isbot": "3.6.13",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@gorakuicu/eslint-config": "workspace:*",
    "@gorakuicu/tsconfig": "workspace:*",
    "@ladle/react": "2.17.2",
    "@remix-run/dev": "1.19.3",
    "@testing-library/react": "14.0.0",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "autoprefixer": "10.4.15",
    "happy-dom": "10.11.2",
    "postcss": "8.4.29",
    "tailwindcss": "3.3.3",
    "tsconfig-paths-webpack-plugin": "4.1.0",
    "vitest": "0.34.3",
    "webpack": "5.88.2"
  }
}
