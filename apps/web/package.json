{
  "name": "@gorakuicu/web",
  "version": "1.0.0",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:remix": "remix build",
    "build:worker": "esbuild ./app/entry.worker.ts --outfile=./public/entry.worker.js --minify --bundle --define:process.env.NODE_ENV='\"production\"'",
    "check": "npm run typecheck && npm run lint",
    "clean:basics": "rimraf .cache storybook-static reports",
    "dev": "run-p dev:*",
    "dev:remix": "remix dev",
    "dev:worker": "esbuild ./app/entry.worker.ts --outfile=./public/entry.worker.js --bundle --define:process.env.NODE_ENV='\"development\"' --watch",
    "format": "prettier --config \"../../.prettierrc.json\" --ignore-path \"../../.prettierignore\" --write \"./**/*.{js,ts,tsx,json,css,md,yml,html,mdx}\"",
    "lint": "eslint -c ./.eslintrc.cjs --ignore-path \"../../.eslintignore\" .",
    "lint:fix": "npm run lint -- --fix",
    "sb": "storybook dev -p 6006",
    "sb:build": "storybook build",
    "start": "remix-serve build",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@gorakuicu/db": "workspace:*",
    "@nextui-org/react": "2.1.13",
    "@remix-pwa/cache": "2.0.12",
    "@remix-pwa/client": "2.0.1",
    "@remix-pwa/push": "1.0.9",
    "@remix-pwa/strategy": "2.1.9",
    "@remix-pwa/sw": "2.1.11",
    "@remix-pwa/sync": "2.0.1",
    "@remix-pwa/worker-runtime": "2.0.7",
    "@remix-run/css-bundle": "2.0.1",
    "@remix-run/node": "2.0.1",
    "@remix-run/react": "2.0.1",
    "@remix-run/serve": "2.0.1",
    "@tailwindcss/typography": "^0.5.10",
    "clsx": "2.0.0",
    "framer-motion": "10.16.4",
    "isbot": "3.7.0",
    "npm-run-all": "4.1.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "4.11.0",
    "remix-utils": "7.1.0",
    "web-push": "3.6.6",
    "workbox-background-sync": "7.0.0",
    "workbox-routing": "7.0.0",
    "workbox-strategies": "7.0.0"
  },
  "devDependencies": {
    "@gorakuicu/eslint-config": "workspace:*",
    "@gorakuicu/tsconfig": "workspace:*",
    "@ngneat/falso": "7.1.1",
    "@remix-run/dev": "2.0.1",
    "@remix-run/testing": "2.0.1",
    "@storybook/addon-a11y": "7.4.6",
    "@storybook/addon-actions": "7.4.6",
    "@storybook/addon-controls": "7.4.6",
    "@storybook/addon-essentials": "7.4.6",
    "@storybook/addon-interactions": "7.4.6",
    "@storybook/addon-links": "7.4.6",
    "@storybook/addons": "7.4.6",
    "@storybook/api": "7.4.6",
    "@storybook/blocks": "7.4.6",
    "@storybook/builder-vite": "7.4.6",
    "@storybook/components": "7.4.6",
    "@storybook/core-events": "7.4.6",
    "@storybook/manager-api": "7.4.6",
    "@storybook/react": "7.4.6",
    "@storybook/react-vite": "7.4.6",
    "@storybook/testing-library": "0.2.2",
    "@storybook/theming": "7.4.6",
    "@testing-library/react": "14.0.0",
    "@types/react": "18.2.28",
    "@types/react-dom": "18.2.13",
    "autoprefixer": "10.4.16",
    "eslint-plugin-perfectionist": "2.2.0",
    "eslint-plugin-storybook": "0.6.15",
    "eslint-plugin-tailwindcss": "3.13.0",
    "postcss": "8.4.31",
    "storybook": "7.4.6",
    "storybook-addon-themes": "6.1.0",
    "storybook-dark-mode": "3.0.1",
    "tailwindcss": "3.3.3",
    "vite": "4.4.11",
    "vite-tsconfig-paths": "4.2.1",
    "@remix-pwa/dev": "2.0.30",
    "remix-pwa": "3.0.18"
  }
}
