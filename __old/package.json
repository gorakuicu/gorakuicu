{
  "name": "gorakuicu",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "FORCE_COLOR=1 turbo dev --cache-dir=./.turbo",
    "build": "FORCE_COLOR=1 turbo build --cache-dir=./.turbo",
    "start": "FORCE_COLOR=1 turbo start",
    "e2e": "FORCE_COLOR=1 turbo e2e --filter site --cache-dir=./.turbo",
    "e2e:headless": "FORCE_COLOR=1 turbo e2e --filter site --headless --cache-dir=./.turbo",
    "component": "FORCE_COLOR=1 turbo component --cache-dir=./.turbo",
    "component:headless": "FORCE_COLOR=1 turbo component --headless --cache-dir=./.turbo",
    "lint": "pnpm formatter && pnpm lint:css && pnpm lint:es",
    "lint:es": "FORCE_COLOR=1 turbo lint:es --cache-dir=./.turbo",
    "lint:ts": "FORCE_COLOR=1 turbo lint:ts --cache-dir=./.turbo",
    "lint:es:fix": "FORCE_COLOR=1 turbo lint:es --cache-dir=./.turbo -- --fix",
    "lint:css": "FORCE_COLOR=1 turbo lint:css --filter site --cache-dir=./.turbo",
    "lint:css:fix": "FORCE_COLOR=1 turbo lint:css --filter site --cache-dir=./.turbo -- --fix",
    "lint:cm": "commitlint --config commitlint.config.cjs --from HEAD",
    "lint:fix": "pnpm formatter && pnpm lint:css:fix && pnpm lint:es:fix",
    "formatter": "prettier --config \"./.prettierrc.json\" --ignore-path \"./.prettierignore\" --write \"./apps/**/*.{js,ts,tsx,json,css,md,yml,html,mdx}\"",
    "cm": "cz",
    "cm:all": "git add . && cz && node ./scripts/git/push.js",
    "cm:stage": "pnpm cm:all && pnpm stage",
    "release": "node ./scripts/git/release.js",
    "release:prod": "node ./scripts/git/release.js production",
    "versions": "node ./scripts/check-versions.js",
    "turbo:prune": "FORCE_COLOR=1 turbo turbo:prune",
    "analyze": "cross-env ANALYZE=true NODE_ENV=production FORCE_COLOR=1 turbo build --cache-dir=./.turbo",
    "size-limit": "pnpm turbo:prune && pnpm dlx size-limit",
    "rm:cache": "rm -rf **/.turbo; rm -rf **/.next",
    "preinstall": "npx -y only-allow pnpm",
    "postinstall": "node scripts/ci/is.js || husky install; node scripts/ci/is.js || npx @evilmartians/lefthook install; node scripts/ci/is.js || npx @evilmartians/lefthook add pre-commit"
  },
  "dependencies": {
    "@builder.io/partytown": "0.8.0",
    "@types/node": "20.3.3",
    "@types/jest": "29.5.2",
    "dotenv": "16.3.1",
    "eslint-config-standard": "17.1.0",
    "module-alias": "2.2.3",
    "pm2": "5.3.0",
    "turbo": "1.10.7",
    "typescript": "5.1.6"
  },
  "devDependencies": {
    "@commitlint/cli": "17.6.6",
    "@commitlint/config-conventional": "17.6.6",
    "@evilmartians/lefthook": "1.4.3",
    "@size-limit/preset-app": "8.2.6",
    "@statoscope/webpack-plugin": "5.26.2",
    "@turbo/gen": "1.10.7",
    "@typescript-eslint/eslint-plugin": "5.60.1",
    "@typescript-eslint/parser": "5.60.1",
    "commitizen": "4.3.0",
    "cross-env": "7.0.3",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.44.0",
    "eslint-config-next": "13.4.7",
    "eslint-config-prettier": "8.8.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-import-resolver-typescript": "3.5.5",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-import-newlines": "1.3.1",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-n": "16.0.1",
    "eslint-plugin-no-loops": "0.3.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "husky": "8.0.3",
    "open": "9.1.0",
    "prettier": "2.8.8",
    "prettier-plugin-tailwindcss": "0.3.0",
    "start-server-and-test": "2.0.0",
    "stylelint": "15.9.0",
    "stylelint-a11y": "1.2.3",
    "stylelint-codeguide": "0.2.0",
    "stylelint-config-clean-order": "5.0.1",
    "stylelint-config-prettier": "9.0.5",
    "stylelint-config-standard": "33.0.0",
    "stylelint-css-modules": "1.2.2",
    "stylelint-css-modules-no-global-scoped-selector": "1.0.2",
    "stylelint-csstree-validator": "3.0.0",
    "stylelint-declaration-block-no-ignored-properties": "2.7.0",
    "stylelint-group-selectors": "1.0.10",
    "stylelint-high-performance-animation": "1.8.0",
    "stylelint-images": "2.0.1",
    "stylelint-prettier": "3.0.0",
    "stylelint-selector-bem-pattern": "3.0.0",
    "stylelint-webpack-plugin": "4.1.1",
    "tsc": "2.0.4"
  },
  "engines": {
    "node": ">=18.16.0",
    "npm": "please-use-pnpm",
    "pnpm": ">=8.6.0"
  },
  "packageManager": "pnpm@8.6.0",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "size-limit": [
    {
      "path": "out",
      "limit": "1.5 MB"
    },
    {
      "path": "out",
      "limit": "1.5 MB",
      "running": "next start"
    },
    {
      "path": "out",
      "limit": "1.5 MB",
      "running": "next build"
    },
    {
      "path": "out",
      "limit": "1.5 MB",
      "running": "next build && next start"
    }
  ]
}
